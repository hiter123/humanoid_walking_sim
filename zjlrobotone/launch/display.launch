<launch>
  <arg
    name="model" />
  <arg
    name="gui"
    default="true" />
  <param
    name="robot_description"
    textfile="$(find zjlrobotone)/urdf/zjlrobotone.urdf" />
  <node
    name="joint_state_publisher_gui"
    pkg="joint_state_publisher_gui"
    type="joint_state_publisher_gui" />
  <param
    name="use_gui"
    value="$(arg gui)" />
    <rosparam param = "source_list">[/topic_m]</rosparam>
  <node
    name="robot_state_publisher"
    pkg="robot_state_publisher"
    type="robot_state_publisher" />
  <node
    name="rviz"
    pkg="rviz"
    type="rviz"
    args="-d $(find zjlrobotone)/urdf.rviz" />
</launch>
